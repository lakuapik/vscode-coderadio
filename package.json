{
  "name": "coderadio",
  "displayName": "Code Radio",
  "description": "Listen 24/7 music designed for coding",
  "keywords": [
    "coderadio",
    "freecodecamp",
    "radio",
    "streaming",
    "music"
  ],
  "publisher": "lakuapik",
  "icon": "icon.png",
  "version": "0.0.2",
  "license": "MIT",
  "engines": {
    "vscode": "^1.50.0"
  },
  "homepage": "https://github.com/lakuapik/vscode-coderadio",
  "repository": {
    "type": "git",
    "url": "https://github.com/lakuapik/vscode-coderadio.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "extensionKind": ["ui"],
    "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "coderadio.play",
        "title": "Code Radio - play"
      },
      {
        "command": "coderadio.stop",
        "title": "Code Radio - stop"
      }
    ],
    "configuration": {
      "title": "Code Radio",
      "properties": {
        "coderadio.vlc_path": {
          "description": "vlc executable path",
          "type": "string"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "package": "yarn lint && yarn compile && vscde package"
  },
  "devDependencies": {
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "typescript": "^4.0.2",
    "vlc-player": "^2.0.0"
  },
  "dependencies": {}
}
